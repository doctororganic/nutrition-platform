{
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.go"
    }
  ],
  "env": {
    "PORT": "8080",
    "HOST": "0.0.0.0",
    "ENVIRONMENT": "production",
    "CORS_ALLOWED_ORIGINS": "*"
  },
  "functions": {
    "main.go": {
      "maxDuration": 30
    }
  }
}
