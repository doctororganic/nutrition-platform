{
  "carb_cycling_system": {
    "description": "نظام تدوير الكربوهيدرات المخصص للأهداف المختلفة",
    "algorithm": {
      "client_assessment": {
        "basic_data": ["age", "height", "weight", "activity_level", "goal", "medical_conditions", "medications", "injuries", "food_preferences"],
        "medical_conditions_priority": {
          "diabetes_insulin_resistance": {
            "recommended_diet": ["low_carb", "mediterranean"],
            "carb_percentage": "20-30%",
            "reason": "تحسين حساسية الأنسولين وخفض سكر الدم"
          },
          "heart_hypertension": {
            "recommended_diet": ["dash", "mediterranean"],
            "carb_percentage": "55%",
            "protein_percentage": "25%",
            "reason": "خفض الدهون والضغط"
          },
          "kidney_disease": {
            "recommended_diet": "controlled_protein",
            "protein_limit": "0.6-0.8g/kg",
            "reason": "تجنب تفاقم الحالة"
          },
          "inflammation": {
            "recommended_diet": "anti_inflammatory",
            "focus": "زيادة أوميغا-3، تقليل السكر",
            "reason": "تقليل الالتهابات"
          }
        }
      }
    },
    "carb_cycling_types": {
      "classic_carb_cycling": {
        "name_ar": "تدوير الكربوهيدرات الكلاسيكي",
        "name_en": "Classic Carb Cycling",
        "description_ar": "نظام يناوب بين أيام عالية ومنخفضة الكربوهيدرات",
        "description_en": "System alternating between high and low carb days",
        "suitable_for": ["weight_loss", "muscle_building", "athletes"],
        "cycle_pattern": {
          "high_carb_days": {
            "frequency": "2 days per week",
            "carb_percentage": 45,
            "protein_percentage": 30,
            "fat_percentage": 25,
            "timing": "training_days",
            "carb_grams_per_kg": "4-6g"
          },
          "moderate_carb_days": {
            "frequency": "3 days per week",
            "carb_percentage": 35,
            "protein_percentage": 35,
            "fat_percentage": 30,
            "timing": "light_training_days",
            "carb_grams_per_kg": "2-3g"
          },
          "low_carb_days": {
            "frequency": "2 days per week",
            "carb_percentage": 20,
            "protein_percentage": 40,
            "fat_percentage": 40,
            "timing": "rest_days",
            "carb_grams_per_kg": "0.5-1g"
          }
        }
      },
      "targeted_carb_cycling": {
        "name_ar": "تدوير الكربوهيدرات المستهدف",
        "name_en": "Targeted Carb Cycling",
        "description_ar": "نظام يركز الكربوهيدرات حول أوقات التمرين",
        "description_en": "System focusing carbs around workout times",
        "suitable_for": ["advanced_athletes", "bodybuilders"],
        "daily_pattern": {
          "pre_workout": {
            "carb_amount": "30-50g",
            "timing": "30-60 minutes before",
            "type": "fast_absorbing"
          },
          "post_workout": {
            "carb_amount": "30-80g",
            "timing": "within 30 minutes",
            "type": "fast_absorbing"
          },
          "rest_of_day": {
            "carb_amount": "minimal",
            "focus": "protein_and_fats"
          }
        }
      }
    },
